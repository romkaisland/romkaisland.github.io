				    if (!isFirst) {
				    	$('#cntloader').attr('src', this._data[this._active].postlink);
				    }
					isFirst = false;
				    $('.galleria-info-title').text(this._data[this._active].title);
				    $('.galleria-info-description').text(this._data[this._active].description);
				});
			});
		</script>
	
